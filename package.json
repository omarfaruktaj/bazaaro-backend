{
  "name": "bazaaro",
  "packageManager": "yarn@4.5.1",
  "scripts": {
    "build": "tsc && tsc-alias ",
    "dev": "ts-node-dev --env-file=.env -r tsconfig-paths/register  src/server.ts",
    "start": "node --env-file=.env dist/server.js",
    "format": "biome format --write src",
    "lint": "biome lint --write  src",
    "lint:fix": "biome lint  --write --unsafe src",
    "format-lint": "biome check --write src",
    "postinstall": "husky"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "envalid": "^8.0.0",
    "express": "^5.0.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.16",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bcryptjs": "^2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "@types/nodemailer": "^6",
    "husky": "^9.1.7",
    "prisma": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
